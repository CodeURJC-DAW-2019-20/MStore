    .card-body
      .tab-content
        #result4.tab-pane.fade.show.active(role='tabpanel')
          pre: code &lt;p&gt;Sample text here...&lt;/p&gt;<br>&lt;p&gt;And another line of sample text here...&lt;/p&gt;
        #html4.tab-pane.fade(role='tabpanel')
          pre.line-numbers
            code(class='lang-html').
              &lt;pre&gt;
                &lt;code&gt;&amp;lt;p&amp;gt;Sample text here...&amp;lt;/p&amp;gt;
                &amp;lt;p&amp;gt;And another line of sample text here...&amp;lt;/p&amp;gt;&lt;/code&gt;
              &lt;/pre&gt;
        #pug4.tab-pane.fade(role='tabpanel')
          pre.line-numbers
            code(class='lang-pug').
              pre
                code
                  &amp;lt;p&amp;gt;Sample text here...&amp;lt;/p&amp;gt;
                  &amp;lt;p&amp;gt;And another line of sample text here...&amp;lt;/p&amp;gt;

  // Code block scrollable
  h2.h5.pt-5.mb-3 Code block scrollable
  .card
    .card-header
      ul(role='tablist').nav.nav-tabs.card-header-tabs
        li.nav-item
